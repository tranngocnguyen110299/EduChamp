<div class="page-banner ovbl-dark" style="background-image:url(@Url.Content((string)ViewBag.Banner));">
    <div class="container">
        <div class="page-banner-entry">
            <h1 class="text-white">@ViewBag.BannerText</h1>
        </div>
    </div>
</div>
<div class="breadcrumb-row">
    <div class="container">
        <ul class="list-inline">
            <li><a href="@Url.Action("Index", "Home")">Home</a></li>
            <li>@ViewBag.BannerText</li>
            @if (ViewBag.Child != null)
            {
                <li>@ViewBag.Child</li>
            }
        </ul>
    </div>
</div>
