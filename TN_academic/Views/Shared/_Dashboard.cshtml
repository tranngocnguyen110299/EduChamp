<script src="~/Content/dashboard/js/functions.js"></script>
<script src="~/Content/dashboard/vendors/chart/chart.min.js"></script>
<script src="~/Content/dashboard/js/admin.js"></script>
<script src='~/Content/dashboard/vendors/calendar/moment.min.js'></script>
<script src='~/Content/dashboard/vendors/calendar/fullcalendar.js'></script>
<script src='~/Content/dashboard/vendors/switcher/switcher.js'></script>

<script>
    $(document).keydown(function (e) {
        if (e.which === 123) {
            return false;
        }
    });
    $(document).bind("contextmenu", function (e) {
        e.preventDefault();

    });

</script>

@RenderSection("ClientScript", false)